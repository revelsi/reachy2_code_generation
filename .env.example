# OpenAI API key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Model to use (optional, defaults to gpt-4o)
MODEL=gpt-4o

# Reachy settings
# Use the IP address of your physical robot for physical mode
REACHY_HOST=localhost

# WebSocket server host and port (optional)
WS_HOST=0.0.0.0
WS_PORT=8765

# Debug mode (optional)
DEBUG=false 