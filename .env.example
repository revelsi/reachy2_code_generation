# OpenAI API key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Model to use (optional, defaults to gpt-4-turbo)
MODEL=gpt-4-turbo

# Reachy robot host (required for connecting to a physical robot)
REACHY_HOST=192.168.1.100

# Whether to use mock implementations (true = use mock implementations, false = connect to real robot)
USE_MOCK=true

# Whether to use a virtual robot (optional, defaults to true)
USE_VIRTUAL=true

# API server host and port (optional)
API_HOST=0.0.0.0
API_PORT=8000

# WebSocket server host and port (optional)
WS_HOST=0.0.0.0
WS_PORT=8000

# Debug mode (optional)
DEBUG=false 