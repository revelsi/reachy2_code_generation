# OpenAI API key (required)
OPENAI_API_KEY=your_openai_api_key_here

# Model to use (optional, defaults to gpt-4-turbo)
MODEL=gpt-4-turbo

# Reachy settings
# Use localhost for virtual mode (Docker container)
# Use the IP address of your physical robot for physical mode
REACHY_HOST=localhost

# API server host and port (optional)
API_HOST=0.0.0.0
API_PORT=8000

# WebSocket server host and port (optional)
WS_HOST=0.0.0.0
WS_PORT=8000

# Debug mode (optional)
DEBUG=false 