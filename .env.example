# OpenAI API key (required)
OPENAI_API_KEY=your_api_key_here

# Models to use (optional)
MODEL=gpt-4.1-mini
EVALUATOR_MODEL=gpt-4.1-nano

# Reachy settings
# Use the IP address of your physical robot for physical mode
REACHY_HOST=localhost

# WebSocket server host and port (optional)
WS_HOST=0.0.0.0
WS_PORT=8765

# Debug mode (optional)
DEBUG=false 